{
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "NetClient.Elastic": "Debug"
      }
    },
    "WriteTo": [
      { 
        "Name": "Console",
        "Args": {
          "outputTemplate": "-> [{Timestamp:HH:mm:ss} {Level:u3} {ElasticApmTraceId} {ElasticApmTransactionId}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}